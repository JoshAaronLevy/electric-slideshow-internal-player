{
  "name": "electric-slideshow-internal-player",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "install:clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "build:clean": "rm -rf dist-electron dist && pnpm build",
